{
	"name":"symfony/framework-standard-edition",
	"description":"The \"Symfony Standard Edition\" distribution",
	"autoload":{
		"psr-0":{
			"":"src/"
		}
	},
	"repositories":[
		{
			"type":"package",
			"package":{
				"name":"formapro/LightOpenID",
				"version":"dev-master",
				"source":{
					"url":"git://github.com/formapro/LightOpenID.git",
					"type":"git",
					"reference":"master"
				}
			}
		},
		{
			"type":"package",
			"package":{
				"name":"RL/Ucaptcha",
				"version":"dev-master",
				"source":{
					"url":"git://gitorious.org/rulinux-engine/ucaptcha.git",
					"type":"git",
					"reference":"master"
				}
			}
		},
		{
			"type":"package",
			"package":{
				"name":"GeSHi/GeSHi",
				"version":"dev-master",
				"source":{
					"url":"https://geshi.svn.sourceforge.net/svnroot/geshi",
					"type":"svn",
					"reference":"branches/RELEASE_1_0_X_STABLE/geshi-1.0.X/"
				}
			}
		},
		{
			"type":"package",
			"package":{
				"name":"RL/phpMathPublisher",
				"version":"dev-master",
				"source":{
					"url":"git://gitorious.org/rulinux-engine/phpmathpublisher.git",
					"type":"git",
					"reference":"master"
				}
			}
		}
	],
	"require":{
		"php":">=5.3.3",
		"symfony/symfony":"2.1.*",
		"doctrine/orm":">=2.2.3,<2.4-dev",
		"doctrine/doctrine-bundle":"1.0.*",
		"twig/extensions":"1.0.*",
		"symfony/assetic-bundle":"2.1.*",
		"symfony/swiftmailer-bundle":"2.1.*",
		"symfony/monolog-bundle":"2.1.*",
		"sensio/distribution-bundle":"2.1.*",
		"sensio/framework-extra-bundle":"2.1.*",
		"sensio/generator-bundle":"2.1.*",
		"jms/security-extra-bundle":"1.2.*",
		"jms/di-extra-bundle":"1.1.*",
		"netpositive/discriminatormapbundle":"*",
		"gregwar/image-bundle":"1.0.0",
		"doctrine/doctrine-fixtures-bundle":"dev-master",
		"formapro/LightOpenID":"dev-master",
		"RL/Ucaptcha":"dev-master",
		"GeSHi/GeSHi":"dev-master",
		"RL/phpMathPublisher":"dev-master"
	},
	"scripts":{
		"post-install-cmd":[
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
		],
		"post-update-cmd":[
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
		]
	},
	"minimum-stability":"dev",
	"extra":{
		"symfony-app-dir":"app",
		"symfony-web-dir":"web",
		"symfony-assets-install":"symlink"
	}
}
