{%extends theme.path('layout.html.twig')%}


{%block title%}
	{{parent()}} - {%trans%}View unconfirmed{%endtrans%}
{%endblock title%}

{%block content%}
<h1>{%trans%}View unconfirmed{%endtrans%}</h1><br>

	{%for thread in threads%}
		{%set section = thread.subsection.section %}
		{%set subsection = thread.subsection %}
		{%include theme.path('unconfirmedItem.html.twig', thread.subsection.section.bundle)%}
	{%endfor%}
{%endblock content%}
