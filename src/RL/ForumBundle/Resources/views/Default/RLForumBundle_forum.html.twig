{% extends theme.path('RLMainBundle', 'layout.html.twig') %}

{%block middle%}
<td colspan="2" class="content">
<ul>
{% for subsection in subsections %}
    <li><a href="{{path('subsection', {'name': subsection.rewrite})}}">{{ subsection.name|e }}</a> - {{ subsection.description|e }}</li>
{% endfor %}
</ul>
</td>
{%endblock%}
